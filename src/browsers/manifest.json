{
    "manifest_version": 2,
    "name": "Gudang Komik Scrapper",
    "version": "1.0",
    "description": "Scrap Targeted website",
    "icons": {
        "48": "icon.png"
    },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "webNavigation",
        "<all_urls>"
    ],
    "content_scripts": [{
        "matches": [
            "*://*.localhost/*",
            "*://*.komikcast.com/*",
            "*://*.manhwaindo.id/*",
            "*://*.manhwaland.bar/*",
            "*://*.komikcastid.com/*"
        ],
        "js": ["dist/main.js"]
    }]
}