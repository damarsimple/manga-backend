{
    "name": "manga-backend",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "dev": "ts-node-dev --transpile-only --no-notify src/server/graphql.ts",
        "seed": "ts-node-dev --transpile-only --no-notify prisma/seed.ts",
        "restore": "ts-node-dev --transpile-only --no-notify prisma/restore.ts",
        "index:sync": "ts-node-dev --transpile-only --no-notify prisma/meili.ts",
        "playground": "ts-node-dev --transpile-only --no-notify src/playground.ts",
        "worker": "ts-node-dev --transpile-only --no-notify src/modules/Worker.ts",
        "scrap": "tsc && node dist/src/scrappers/runner.js",
        "testscrap": "tsc && node dist/src/scrappers/test.js",
        "start": "tsc && node  dist/src/server/graphql.js",
        "testdo": "prisma generate && tsc && node  dist/src/scripts/jobs/migrate-to-do.js",
        "queuesize": "node  dist/src/scripts/jobs/check-size-migration.js",
        "build": "tsc",
        "cron": "tsc && TS_NODE=true NODE_OPTIONS=\"-r ts-node/register\" node src/scripts/cron.ts",
        "ext:compile": "WEBPACK=true webpack",
        "ext:watch": "WEBPACK=true webpack --watch"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.52.0",
