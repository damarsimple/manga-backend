        "@types/axios": "^0.14.0",
        "@types/graphql-upload": "^8.0.11",
        "@types/jsdom": "^16.2.14",
        "@types/lodash": "^4.14.182",
        "@types/moment": "^2.13.0",
        "@types/node": "^17.0.18",
        "@types/sharp": "^0.29.5",
        "JSONStream": "^1.3.5",
        "apollo-server": "^3.6.2",
        "apollo-server-express": "^3.6.3",
        "async": "^3.2.3",
        "async-es": "^3.2.3",
        "axios": "^0.25.0",
        "axios-socks5-agent": "^1.0.5",
        "bcrypt": "^5.0.1",
